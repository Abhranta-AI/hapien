{
  "name": "hapien-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.26.0",
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "pnpm --filter @hapien/web dev",
    "dev:mobile": "pnpm --filter @hapien/mobile start",
    "build": "turbo build",
    "build:web": "pnpm --filter @hapien/web build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "vercel-build": "pnpm --filter @hapien/web build"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "next": "14.2.5"
  }
}
